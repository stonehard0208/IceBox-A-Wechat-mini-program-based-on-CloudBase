/**index.wxss**/
page {
  background-color: #fff;
}
.container {
  background-color: #fff;
  width: 100%;
  /* padding: 60rpx 80rpx 0; */
  padding: 6vh 80rpx 0;
}
.top, .middle, .connect, .bottom{
  background-color: rgb(213, 227, 239);
  border: 20rpx solid rgb(1, 68, 99);
}
.top{
  width: 100%;
  /* height: 400rpx; */
  height: 35vh;
  border-top-left-radius: 50rpx;
  border-top-right-radius: 50rpx;
}
.middle{
  width: 100%;
  /* height: 600rpx; */
  height: 45vh;
  border-top: 0;
  border-bottom-left-radius: 50rpx;
  border-bottom-right-radius: 50rpx;
}
.bottom{
  width: 80%; 
  /* height: 50rpx; */
  height: 4vh;
  border-top: 0;
}
.tag{
  width: 141rpx;
  height: 178rpx;
  margin: 105rpx 120rpx;
}
.handleup{
  width: 170rpx;
  height: 170rpx;
  float: right;
  margin-top: 120rpx;
  margin-right: 10rpx;
}
.handledown{
  width: 170rpx;
  height: 170rpx;
  float: right;
  margin-top: 190rpx;
  margin-right: 10rpx;
}
.light1, .light2{
  background-color: rgb(236, 240, 249);
  float: left;
}
.light1{
  width: 45rpx;
  height: 220rpx;
  border-radius: 50rpx;
  margin-top: 100rpx;
  margin-left: 110rpx;
}
.light2{
  width: 45rpx;
  height: 45rpx;
  border-radius: 100%;
  margin-top: 360rpx;
  margin-left: -45rpx;
}